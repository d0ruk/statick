{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 7,
  },
  "env": {
    "es6": true,
    "node": true
  },
  rules: {
    "indent": [
      2,
      2
    ],
    "curly": [
      2,
      "multi-line"
    ],
    "import/first": "off",
    "comma-dangle": "off",
    "no-unused-vars": "error",
    "no-use-before-define": "off",
    "no-undef": "error",
    "object-curly-spacing": "off",
    "no-console": "warn",
    "no-extra-semi": "warn",
    "no-irregular-whitespace": "warn",
    "no-unexpected-multiline": "warn",
    "no-unreachable": "error"
  }
}
